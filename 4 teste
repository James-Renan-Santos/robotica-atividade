#include <iostream>
using namespace std;

int main() {
    int inteiro = 5 / 2;
    float pontoFlutuante = 5.0f / 2.0f;
    double pontoDuplo = 5.0 / 2.0;

    cout << "int: 5 / 2 = " << inteiro << endl;
    cout << "float: 5.0f / 2.0f = " << pontoFlutuante << endl;
    cout << "double: 5.0 / 2.0 = " << pontoDuplo << endl;

    return 0;
}
