#include <iostream>
using namespace std;

int main() {
    int inteiro = 10;
    float decimalSimples = 3.14f;
    double decimalDuplo = 3.1415926535;
    char caractere = 'A';
    bool booleano = true;
    short curto = 100;
    long longo = 100000;
    long long muitoLongo = 10000000000LL;

    cout << "int: " << inteiro << endl;
    cout << "float: " << decimalSimples << endl;
    cout << "double: " << decimalDuplo << endl;
    cout << "char: " << caractere << endl;
    cout << "bool: " << booleano << endl;
    cout << "short: " << curto << endl;
    cout << "long: " << longo << endl;
    cout << "long long: " << muitoLongo << endl;

    return 0;
}
